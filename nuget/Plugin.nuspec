    <file src="..\src\Plugin.FileUploader\bin\Release\Plugin.FileUploader.dll" target="lib\netstandard1.4\Plugin.FileUploader.dll" />
    <file src="..\src\Plugin.FileUploader\bin\Release\Plugin.FileUploader.xml" target="lib\netstandard1.4\Plugin.FileUploader.xml" />
    <file src="..\src\Plugin.FileUploader.Abstractions\bin\Release\Plugin.FileUploader.Abstractions.dll" target="lib\netstandard1.4\Plugin.FileUploader.Abstractions.dll" />
    <file src="..\src\Plugin.FileUploader.Abstractions\bin\Release\Plugin.FileUploader.Abstractions.xml" target="lib\netstandard1.4\Plugin.FileUploader.Abstractions.xml" />

    <!-- iOS reference assemblies -->
    <file src="..\src\Plugin.FileUploader.iOS\bin\iPhone\Release\Plugin.FileUploader.dll" target="lib\Xamarin.iOS10\Plugin.FileUploader.dll" />
    <file src="..\src\Plugin.FileUploader.iOS\bin\iPhone\Release\Plugin.FileUploader.xml" target="lib\Xamarin.iOS10\Plugin.FileUploader.xml" />

    <!-- Android reference assemblies -->
    <file src="..\src\Plugin.FileUploader.Android\bin\Release\Plugin.FileUploader.dll" target="lib\MonoAndroid10\Plugin.FileUploader.dll" />
    <file src="..\src\Plugin.FileUploader.Android\bin\Release\Plugin.FileUploader.xml" target="lib\MonoAndroid10\Plugin.FileUploader.xml" />

  </files>
