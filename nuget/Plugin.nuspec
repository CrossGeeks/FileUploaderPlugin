<?xml version="1.0"?>
<package >
  <metadata minClientVersion="2.8.1">
    <id>Plugin.FileUploader</id>
    <version>1.0.1</version>
    <title>FileUploader Plugin for Xamarin iOS and Android</title>
    <authors>Rendy Del Rosario</authors>
    <owners>crossgeeks,rdelrosario</owners>
    <licenseUrl>https://raw.githubusercontent.com/CrossGeeks/FileUploaderPlugin/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/CrossGeeks/FileUploaderPlugin</projectUrl>
    <iconUrl>https://github.com/CrossGeeks/FileUploaderPlugin/blob/master/art/icon.png?raw=true</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Upload files across Xamarin.iOS and Xamarin.Android</description>
    <summary>Upload files across Xamarin.iOS and Xamarin.Android</summary>
    <releaseNotes>Support for uploading files on Android and iOS</releaseNotes>
    <copyright>Copyright 2017</copyright>
    <tags>iOS,Android,File uploading</tags>
    <dependencies>
        <group targetFramework="MonoAndroid">
            <dependency id="modernhttpclient" version="2.4.2" />
        </group>
    </dependencies>
  </metadata>
  <files>
     <file src="..\src\Plugin.FileUploader\bin\Release\Plugin.FileUploader.*" target="lib\portable-net45+wp80+win8+wpa81" />
