<?xml version="1.0"?>
<package >
  <metadata minClientVersion="2.8.1">
    <id>Plugin.FileUploader</id>
    <version>1.1.3</version>
    <title>File Uploader Plugin for Xamarin</title>
    <authors>Rendy Del Rosario</authors>
    <owners>crossgeeks</owners>
    <licenseUrl>https://raw.githubusercontent.com/CrossGeeks/FileUploaderPlugin/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/CrossGeeks/FileUploaderPlugin</projectUrl>
    <iconUrl>https://github.com/CrossGeeks/FileUploaderPlugin/blob/master/art/icon.png?raw=true</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Upload files using multipart request across Xamarin.iOS, Xamarin.Android, Xamarin.Mac, tvOS and watchOS</description>
    <summary>Upload files using multipart request across Xamarin.iOS, Xamarin.Android, Xamarin.Mac, tvOS and watchOS</summary>
    <copyright>Copyright 2017</copyright>
    <tags>iOS,Android,files,upload,multipart</tags>
     <dependencies>
      <group targetFramework="netstandard1.0">
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="MonoAndroid10">
        <dependency id="modernhttpclient" version="2.4.2" />
      </group>
      <group targetFramework="Xamarin.iOS10">
      </group>
      <group targetFramework="Xamarin.Mac20">
      </group>
      <group targetFramework="portable-net45+win+wpa81+wp80">
      </group>
      <group targetFramework="xamarintvos">
      </group>
      <group targetFramework="xamarinwatchos">
      </group>
    </dependencies>
    <releaseNotes>Mac, tvOS and watchOS support</releaseNotes>
  </metadata>
  <files>
        <!--NetStandard/PCL-->
        <file src="..\src\Plugin.FileUploader\bin\Release\netstandard1.0\Plugin.FileUploader.*" target="lib\netstandard1.0" />
        <!--Xamarin.Android-->
        <file src="..\src\Plugin.FileUploader.Android\bin\Release\Plugin.FileUploader.*" target="lib\MonoAndroid10" />
        <!--Xamarin.iOS-->
        <file src="..\src\Plugin.FileUploader.iOS\bin\iPhone\Release\Plugin.FileUploader.*" target="lib\Xamarin.iOS10" />
        <!--Xamarin.Mac-->
        <file src="..\src\Plugin.FileUploader.Mac\bin\Release\Plugin.FileUploader.*" target="lib\Xamarin.Mac20" />
        <!--tvOS-->
        <file src="..\src\Plugin.FileUploader.tvOS\bin\Release\Plugin.FileUploader.*" target="lib\XamarinTVOS" />
        <!--watchos-->
        <file src="..\src\Plugin.FileUploader.watchOS\bin\Release\Plugin.FileUploader.*" target="lib\XamarinWatchOS" />
  </files>
</package>